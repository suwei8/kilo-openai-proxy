# 监听端口
PORT=8033

# 是否无头模式（false 会弹出浏览器窗口
HEADLESS=false

# Playwright 持久化用户目录（保存登录态）
USER_DATA_DIR=.userdata

# Gemini WebUI 地址
GEMINI_URL=https://gemini.google.com/app

# 页面初始化等待时间(ms)
WAIT_READY_MS=5000

# 最大回答等待时间(ms)
MAX_ANSWER_MS=180000

# 稳定窗口 (ms)，答案在该时间内未更新则认为完成
STABLE_MS=1800

# 把点击按钮兜底彻底关掉，只靠 “回车提交”：   false=默认稳妥模式
FORCE_ENTER_ONLY=false   

# Gemini 的输入框和发送按钮的时间延迟，写入后立刻检查容易“判太早”。给写入后加一个固定稳定等待更�?
AFTER_INPUT_SETTLE_MS=800





# KiloCode 请求体处理开关


# 总开关：KILO_SANITIZE_ON
# true | 1 | on | yes | y → 开启清洗

# false | 0 | off | no | n → 关闭清洗，原样透传
# （布尔解析规则在 flag() 里已写好，兼容多种写法。）
KILO_SANITIZE_ON=false

# 模式：KILO_SANITIZE_MODE
# tools-minimal（默认：最小必要清洗）
# tools-off（去工具标签）
# passthrough（即使开关开着，也逻辑透传）
KILO_SANITIZE_MODE=tools-minimal

KILO_KEEP_TAGS=read_file,apply_diff,write_to_file,execute_command
KILO_STRIP_ENV=true
KILO_STRIP_STREAM_OPTIONS=true
KILO_STRIP_MODEL=true
KILO_BLOCK_MODELS=gemini-2.5-flash,gemini-webui
KILO_PRUNE_TOP_FIELDS=false
KILO_LANG=zh
